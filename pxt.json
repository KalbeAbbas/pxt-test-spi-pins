                    "eddystone_url": 0,
                    "eddystone_uid": 0,
                    "open": 0,
                    "pairing_mode": 0,
                    "whitelist": 1,
                    "security_level": "SECURITY_MODE_ENCRYPTION_NO_MITM",
                    "partial_flashing": 1,
                    "enabled": 0
                }
            }
        }
    },
	
    "panic_on_heap_full": 0,
	

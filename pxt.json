{
    "name": "CS11",
    "version": "0.1.0",
    "description": "XinaBox CS11 SD Card: SD Card functionality.",
    "license": "MIT",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "README.md",
        "main.ts",
        "sdmm.cpp",
        "cs11.ts",
        "cs11.cpp",
        "diskio.h",
        "ff.cpp",
        "ff.h",
